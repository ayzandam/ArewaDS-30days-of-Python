    "msg = [\"How are you\", \"Good day\", \"Good morning\", \"I am great\", \"Thank you\"]\n",
    "send_msg = []\n",
    "\n",
    "def send_message(msg):\n",
    "    for m in msg:\n",
    "        send_msg.append(m)\n",
    "\n",
    "send_message(msg)\n",
    "print(\"Messages to be sent:\")\n",
    "print(msg)\n",
    "print(\"Messages archived\")\n",
    "print(send_msg)"
   ]
